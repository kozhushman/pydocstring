{
	"name": "pydocstring",
	"displayName": "python docstrings",
	"description": "generate python docstrings for classes and methods",
	"version": "0.0.8",
	"publisher": "kopub",
	"license": "SEE LICENSE IN LICENSE",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"python",
		"docstring",
		"documentation"
	],
	"activationEvents": [
		"onCommand:pydocstring.addDocstring"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pydocstring.addDocstring",
				"title": "pydocstring"
			}
		]
	},
	"scripts": {
		"refreshVSToken": "vsts-npm-auth -config .npmrc",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kozhushman/pydocstring"
	}
}
